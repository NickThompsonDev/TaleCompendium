{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "https://cloud.talecompendium.com/$1",
      "has": [
        {
          "type": "host",
          "value": "cloud.talecompendium.com"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/$1",
      "has": [
        {
          "type": "host",
          "value": "talecompendium.com"
        }
      ]
    }
  ]
}
